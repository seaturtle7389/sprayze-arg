<tr id="<%= dom_id adjustment %>" class="whitespace-nowrap">
  <td class="px-6 py-4 text-sm text-gray-500 text-center"><%= adjustment.created_at.strftime("%-m/%-d/%y %I:%M %p %Z") %></td>
  <td class="px-6 py-4 text-sm text-gray-500 text-center"><%= adjustment.amount %></td>
  <td class="px-6 py-4 text-sm text-gray-500 text-center"><%= adjustment.historical_value %></td>
  <td class="px-6 py-4 text-sm text-gray-500 text-center"><%= adjustment.user != nil ? adjustment.user.try(:discord_tag) : "Automatic" %></td>
</tr>
